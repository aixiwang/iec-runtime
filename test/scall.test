# OBJ I/O Configuration Segment
iocs_update_interval 8000000
iocs_ldi_count 0
iocs_ldo_count 0
iocs_lai_count 0
iocs_lao_count 0
iocs_rdi_count 0
iocs_rdo_count 0
iocs_rai_count 0
iocs_rao_count 0

# OBJ Servo Configuration Segment
scs_axis_count 1
scs_update_interval 8000000

acs_name     axis1
acs_id       1
acs_type     AXIS_TYPE_FINITE
acs_combined AXIS_INDEPENDENT
acs_opmode   OPMODE_POS
acs_min_pos  0.0
acs_max_pos  180.0
acs_max_vel  100.0
acs_max_acc  20.0
acs_max_dec  20.0
acs_max_jerk 10.0

# OBJ PLC Task List Segment
plc_task_count 1

# OBJ PLC Task Description Segment
tds_name         task1
tds_priority     80
tds_type         TASK_TYPE_INTERVAL
tds_signal       0
tds_interval     100000000
tds_sp_size      256
tds_cs_size      4
tds_pou_count    1
tds_const_count  3
tds_global_count 3
tds_inst_count   12

# OBJ POU Description Segment
pds_name         main
pds_input_count  0
pds_inout_count  0
pds_output_count 0
pds_local_count  8
pds_entry        0

# OBJ PLC Task Constant Segment
K TDOUBLE 16.0
K TINT 7
K TINT 3

# OBJ PLC Task Global Variables Segment
G TDOUBLE 0
G TINT 0
G TINT 0

# OBJ PLC Task Code Segment
I OP_KLOAD 0 0         # R(0) <-- K(0)
I OP_SCALL 0 SFUN_SQRT # R(0)=REGI; R(1)=REGO
I OP_GSTORE 1 0        # R(1) --> G(0)

I OP_KLOAD 2 1         # R(2) <-- K(1)
I OP_KLOAD 3 2         # R(3) <-- K(2)
I OP_SCALL 2 SFUN_MOD  # R(2,3)=REGI; R(4)=REGO
I OP_GSTORE 4 1        # R(4) --> G(1)

I OP_KLOAD 5 2         # R(5) <-- K(2)
I OP_KLOAD 6 2         # R(6) <-- K(2)
I OP_SCALL 5 SFUN_EXPT # R(5,6)=REGI; R(7)=REGO
I OP_GSTORE 7 2        # R(7) --> G(2)
I OP_HALT 0 0 0        # Halt
